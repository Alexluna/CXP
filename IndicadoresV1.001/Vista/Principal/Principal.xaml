        <Grid.RowDefinitions>
            <RowDefinition Height="20" />
            <RowDefinition Height="30" />
            <RowDefinition Height="20" />
            <RowDefinition Height="30" />
            <RowDefinition Height="136*" />
            <RowDefinition Height="20" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="20" />
            <ColumnDefinition Width="190*" />
            <ColumnDefinition Width="150" />
            <ColumnDefinition Width="190*" />
            <ColumnDefinition Width="20" />
        </Grid.ColumnDefinitions>
        <Button Content="CXP" Grid.Column="2" Grid.Row="1"  Name="cxp" Click="cxp_Click" />
        <Button Content="CRU" Grid.Column="2" Grid.Row="3"  Name="cru" Click="cru_Click" />
    </Grid>
</UserControl>
